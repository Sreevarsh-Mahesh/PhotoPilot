# Example secrets file for Streamlit Cloud
# Copy this to .streamlit/secrets.toml and fill in your values
# DO NOT commit secrets.toml to Git!

# AWS credentials (if using S3 for model hosting)
[aws]
access_key_id = "YOUR_AWS_ACCESS_KEY"
secret_access_key = "YOUR_AWS_SECRET_KEY"
region = "us-east-1"
bucket = "your-model-bucket"

# Google Drive (if using for model hosting)
[gdrive]
file_id = "YOUR_GOOGLE_DRIVE_FILE_ID"

# Hugging Face (if using HF Hub)
[huggingface]
token = "YOUR_HF_TOKEN"
repo_id = "YOUR_USERNAME/photopilot-model"

# Custom model path (if hosted externally)
[model]
url = "https://your-cdn.com/best_model.pth"
local_path = "checkpoints/best_model.pth"
